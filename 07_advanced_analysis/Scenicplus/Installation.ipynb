   "cell_type": "code",
   "execution_count": null,
   "id": "347188b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "git clone https://github.com/aertslab/scenicplus.git\n",
    "cd scenicplus\n",
    "pip install ."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
